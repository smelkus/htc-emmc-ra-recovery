Compiled in gingerbread for the supersonic. 05/17/2012 smelkus

example boardconfig.mk

TARGET_BOARD_PLATFORM := qsd8k
TARGET_BOARD_PLATFORM_GPU := qcom-adreno200
TARGET_GLOBAL_CFLAGS += -mfpu=neon -mfloat-abi=softfp
TARGET_GLOBAL_CPPFLAGS += -mfpu=neon -mfloat-abi=softfp
TARGET_CPU_ABI := armeabi-v7a
TARGET_CPU_ABI2 := armeabi
TARGET_ARCH_VARIANT := armv7-a-neon
ARCH_ARM_HAVE_TLS_REGISTER := true

# see android.mk for these
#TARGET_HAS_WIMAX := true
#BOARD_USE_CUSTOM_FONT := true
#ENABLE_TOUCH_SCROLLING := true
#TARGET_USES_MTD := true

TARGET_BOOTLOADER_BOARD_NAME := armv7neon
